{
	"name": "Custom Client Solutions",
	"version": "0.0.1",
	"scripts": {
		"ng": "ng",
		"ng s": "ng s --host 0.0.0.0 --watch",
		"prestart": "node aspnetcore-https",
		"start": "run-script-os",
		"start:windows": "ng serve --port 44436 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
		"start:default": "ng serve --port 44436 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "19.2.18",
		"@angular/cdk": "19.2.19",
		"@angular/common": "19.2.18",
		"@angular/compiler": "19.2.18",
		"@angular/core": "19.2.18",
		"@angular/forms": "19.2.18",
		"@angular/localize": "19.2.18",
		"@angular/material": "19.2.19",
		"@angular/platform-browser": "19.2.18",
		"@angular/platform-browser-dynamic": "19.2.18",
		"@angular/router": "19.2.18",
		"@emailjs/browser": "^4.3.3",
		"@ng-bootstrap/ng-bootstrap": "^17.0.0",
		"@popperjs/core": "^2.11.8",
		"bootstrap": "^5.3.3",
		"bootstrap-icons": "^1.11.3",
		"run-script-os": "1.1.6",
		"rxjs": "^7.8.1",
		"tslib": "^2.6.3",
		"zone.js": "0.15.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "19.2.20",
		"@angular/cli": "^19.2.20",
		"@angular/compiler-cli": "19.2.18",
		"@types/jasmine": "~5.1.0",
		"@types/node": "^22.0.0",
		"jasmine-core": "~5.4.0",
		"karma": "~6.4.4",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.1",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"typescript": "5.8.3"
	}
